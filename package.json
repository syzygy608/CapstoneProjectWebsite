{
  "name": "vite-js-tailwindcss-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "npx tailwindcss -i ./src/css/index.css -o ./src/css/tailwind.css --minify && vite",
    "css": "npx tailwindcss -i ./src/css/index.css -o ./src/css/tailwind.css --minify --watch",
    "build": "vite build",
    "preview": "vite preview --port 3000"
  },
  "dependencies": {
    "axios": "^1.7.8",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.3",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.1",
    "vite": "^2.9.9"
  }
}
